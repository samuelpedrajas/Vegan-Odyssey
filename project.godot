;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[android]

modules="org/godotengine/godot/GodotAdMob,org/godotengine/godot/GodotShare"

[application]

config/name="Vegan Odyssey"
run/main_scene="res://scenes/loading_screen.tscn"
boot_splash/image="res://images/loading_screen/splash.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0, 0, 0, 0 )
config/icon="res://icon.png"

[autoload]

admob="*res://scripts/autoload/admob.gd"
game="*res://scripts/autoload/game.gd"
game_loader="*res://scripts/autoload/game_loader.gd"

[display]

window/size/width=1080
window/size/height=1920
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
window/handheld/orientation="portrait"
window/handheld/emulate_touchscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[gdnative]

singletons=[  ]

[importer_defaults]

texture={
"flags/filter": true,
"flags/mipmaps": true
}

[input]

click=[ Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"button_mask":0,"position":Vector2( 0, 0 ),"global_position":Vector2( 0, 0 ),"factor":1.0,"button_index":1,"pressed":false,"doubleclick":false,"script":null)
 ]

[rendering]

quality/reflections/texture_array_reflections=false
environment/default_clear_color=Color( 0.65098, 0.65098, 0.65098, 1 )
