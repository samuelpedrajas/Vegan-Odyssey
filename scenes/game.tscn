script/source = "extends Control


func _enter_tree():
	game.setup()


func _ready():
	if game.cfg.DEBUG_MODE:
		game.board_layer.debug_func()
	elif !game.savegame.file_exists($cfg.SAVE_GAME_PATH):
		game.board_layer.spawn_token(null, 1, false)
	else:
		game.load_game()

	# show banner or try again
	if admob.is_banner_loaded:
		admob.showBanner()
	else:
		$admob.start()

	$debate_layer.init(game.current_max)

	$\"/root\".set_disable_input(false)


func _on_admob_timeout():
	print(\"BANNER TIMEOUT: \", admob.is_banner_loaded)
	if admob.is_banner_loaded:
		admob.showBanner()
		$admob.stop()
	else:
		admob.loadBanner()
"

