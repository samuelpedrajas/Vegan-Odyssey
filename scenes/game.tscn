script/source = "extends TextureButton


var btns = []

signal unpost_tutorial


func unpost(n):
	if n != \"4\":
		# hide aura
		for btn in btns:
			if btn.get_name() == \"excuses\":
				game.hud_layer.glow_stop()
		$black_anim.play_backwards(\"open\")
		yield($black_anim, \"animation_finished\")
		var lower_buttons = get_parent()
		lower_buttons.move_child(self, lower_buttons.get_child_count())
		btns = []
		hide()


func _move_btns():
	for btn in btns:
		var lower_buttons = get_parent()
		var n = lower_buttons.get_child_count()
		lower_buttons.move_child(btn, n)


func show_bg(n):
	if n == \"4\":
		return
	if n == \"2\":
		var btn = game.hud_layer.get_node(\"hud/lower_buttons/excuses\")
		btns = [btn]
		game.hud_layer.glow_excuses()
	elif n == \"3\":
		btns = [
			game.hud_layer.get_node(\"hud/lower_buttons/reset\"),
			game.hud_layer.get_node(\"hud/lower_buttons/broccoli\")
		]
	_move_btns()
	show()
	$black_anim.play(\"open\")


func _on_black_bg_pressed():
	emit_signal(\"unpost_tutorial\")
"

