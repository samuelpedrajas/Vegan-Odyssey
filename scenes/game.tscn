script/source = "extends Control


func _enter_tree():
	$\"popup_layer/effects/black\".show()
	game.setup()


func _ready():
	$save_game.set_wait_time(cfg.ANIMATION_TIME + 0.2)
	game.resizer.rescale_all()
	game.start_game()


#func _on_admob_timeout():
#	print(\"BANNER TIMEOUT: \", admob.is_banner_loaded)
#	if not admob.is_banner_loaded:
#		admob.loadBanner()
#	else:
#		$admob.stop()


func rescale(s):
	var h = admob.getHeight()
	var screen_size = get_viewport().get_visible_rect().size

	# rescale buttons
	var controls = [
		$\"hud_layer/hud/lower_buttons/menu\",
		$\"hud_layer/hud/lower_buttons/reset\",
		$\"hud_layer/hud/lower_buttons/excuses\",
		$\"hud_layer/hud/lower_buttons/broccoli\"
	]

	for c in controls:
		c.set_scale(Vector2(s, s))
		c.set_position(
			Vector2(
				c.get_position().x,
				(screen_size.y - h) - (cfg.LOW_BTN_MARGIN + c.get_size().y)
			)
		)

	# reposition bottom_border
	var bottom_border = $\"hud_layer/hud/bottom_border\"
	var upper_part = 52.0
	var half_btn_y = controls[0].get_size().y * s / 2.0
	bottom_border.set_position(
		Vector2(
			bottom_border.get_position().x,
			(screen_size.y - h) - cfg.LOW_BTN_MARGIN - upper_part - half_btn_y
		)
	)

	# reposition and rescale board parts
	var board = $board_layer/board
	var board_size = board.get_size()
	var ideal_position = Vector2(
		screen_size.x * 0.5,
		screen_size.y * cfg.BOARD_POSITION
	)
	var real_position = ideal_position
	var red_line = screen_size.y - h - cfg.LOW_BTN_MARGIN - controls[0].get_size().y * s - cfg.HIGH_BTN_MARGIN

	# adjust
	var over_buttons = ideal_position.y + board_size.y * s / 2.0 > red_line
	var over_top = ideal_position.y - (board_size.y / 2.0 + $dialog_layer/text.get_size().y) * s < 0.0
	if over_buttons:
		real_position.y = red_line - board_size.y * s / 2.0
	elif over_top:
		real_position.y = s * ($dialog_layer/text.get_size().y + board_size.y / 2.0)

	# move and scale board
	var board_scaled_y = board_size.y * s
	board.rescale(s)
	board.set_position(real_position - Vector2(board_size.x / 2.0, board_scaled_y / 2.0))

	# move and scale dying tokens
	var dying_tokens = $dying_tokens/tokens
	var dying_tokens_size = dying_tokens.get_size()
	dying_tokens.rescale(s)
	dying_tokens.set_position(real_position - Vector2(dying_tokens_size.x / 2.0, dying_tokens_size.y * s / 2.0))

	# move and scale text and girls
	var board_parts = [
		$dialog_layer/text,
		$debate_layer/girls
	]

	for c in board_parts:
		c.rescale(s)
		c.set_position(Vector2(
			real_position.x - c.get_size().x / 2.0,
			real_position.y - board_scaled_y / 2.0 - c.get_size().y
		))

	# move and scale effects
	var distance_to_button = 20
	var c = $effects_layer/rewarded_animation/c
	c.set_scale(Vector2(s, s))
	c.set_position(Vector2(c.get_position().x, red_line - distance_to_button))


var max_attempts = 5
var current_attempts = 0


func is_moving():
	# wait until all tokens are stopped
	for t in game.board_layer.matrix.values():
		if not t.is_stopped() or t.animation.is_playing():
			return true
	return false


func _on_save_game_timeout():
	current_attempts += 1
	if current_attempts == max_attempts or not is_moving():
		game.save_game()
		current_attempts = 0
	elif $save_game.is_stopped():
		$save_game.start()
"

