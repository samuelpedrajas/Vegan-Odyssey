script/source = "extends TextureButton


var btn = null

signal unpost_tutorial


func unpost(n):
	if n != \"4\":
		$black_anim.play_backwards(\"open\")
		yield($black_anim, \"animation_finished\")
		var lower_buttons = get_parent()
		lower_buttons.move_child(self, lower_buttons.get_child_count())
		btn = null
		hide()


func _move_btn():
	if btn == null:
		return
	var lower_buttons = get_parent()
	var n = lower_buttons.get_child_count()
	lower_buttons.move_child(btn, n)


func show_bg(n):
	if n == \"4\":
		return
	if n == \"2\":
		btn = game.hud_layer.get_node(\"hud/lower_buttons/excuses\")
	elif n == \"3\":
		btn = game.hud_layer.get_node(\"hud/lower_buttons/broccoli\")
	_move_btn()
	show()
	$black_anim.play(\"open\")


func _on_black_bg_pressed():
	emit_signal(\"unpost_tutorial\")
"

